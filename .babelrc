 {
   "ignore" : [
      "src/**/__tests__/**/*.test.js"
   ],
    "only" : [
                "!src/**/__tests__/**/*.test.js",
                "*.js",
                "*.jsx",
                "*.es6"
             ],
    "plugins" : [       
        "transform-decorators-legacy",
      [ "transform-react-remove-prop-types", {
         "mode": "wrap",
         "ignoreFilenames":["node_modules"]
      }],
      [ "transform-runtime", {
         "helpers": true,
         "polyfill": false,
         "regenerator": false,
         "moduleName": "babel-runtime"
      }]
    ],            
    "presets" : [
                   "es2015",
                   "react",
                   "stage-2"
                ]
 }
