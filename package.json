    "big.js": "^3.1.3",
    "gl-matrix": "^2.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "reflux": "^5.0.4"
  },
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.7.6",
      "big.js": "npm:big.js@^3.1.3",
