    "babel-loader": "^8.0.6",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
